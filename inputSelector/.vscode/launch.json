{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "fulldb-query-orbit-1388",
      "type": "python",
      "request": "launch",
      "stopOnEntry": false,
      "pythonPath": "${config.python.pythonPath}",
      "program": "${file}",
      "cwd": "${workspaceRoot}",
      "debugOptions": [
        "WaitOnAbnormalExit",
        "WaitOnNormalExit",
        "RedirectOutput"
      ],
      "args": [
        "accessist.sqlite",
        "-o",
        "1388"
      ]
    },
    {
      "name": "fulldb-build",
      "type": "python",
      "request": "launch",
      "stopOnEntry": false,
      "pythonPath": "${config.python.pythonPath}",
      "program": "${file}",
      "cwd": "${workspaceRoot}",
      "debugOptions": [
        "WaitOnAbnormalExit",
        "WaitOnNormalExit",
        "RedirectOutput"
      ],
      "args": [
        "-o",
        "-q",
        "-v",
        "--trace=trace.txt",
        "accesslist.sqlite",
        "data/Orbit_Path_Time.txt.gz",
        "data/MODIS.list.gz",
        "data/ASTER.list.gz",
        "data/MOPITT.list.gz",
        "data/MISR.list.gz",
        "data/CERES.list.gz"
      ]
    },
    {
      "name": "oneorbit-build",
      "type": "python",
      "request": "launch",
      "stopOnEntry": false,
      "pythonPath": "${config.python.pythonPath}",
      "program": "${file}",
      "cwd": "${workspaceRoot}",
      "debugOptions": [
        "WaitOnAbnormalExit",
        "WaitOnNormalExit",
        "RedirectOutput"
      ],
      "args": [
        "-o",
        "-v",
        "--trace=trace.txt",
        "oneorbit.sqlite",
        "data/Orbit_Path_Time.txt.gz",
        "testdata/one-orbit-files.txt.gz"
      ]
